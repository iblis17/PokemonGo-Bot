{
    "websocket_server": false,
    "heartbeat_threshold": 10,
    "enable_social": false,
    "live_config_update": {
      "enabled": true,
      "tasks_only": true
    },
    "tasks": [
      {
        "type": "HandleSoftBan"
      },
      {
        "type": "UpdateLiveStats",
        "config": {
          "enabled": true,
          "min_interval": 10,
          "stats": ["username", "uptime", "stardust_earned", "xp_earned", "xp_per_hour", "stops_visited"],
          "terminal_log": true,
          "terminal_title": true
        }
      },
      {
        "type": "UpdateLiveInventory",
        "config": {
          "enabled": false,
          "min_interval": 120,
          "show_all_multiple_lines": false,
          "items": ["pokemon_bag", "space_info", "pokeballs", "greatballs", "ultraballs", "razzberries", "luckyegg"]
        }
      },
      { "type": "UpdateWebInventory",
        "config": {
          "enabled": false
        }
      },
      {
        "type": "IncubateEggs",
        "config": {
          "enabled": true,
          "infinite_longer_eggs_first": false,
          "breakable_longer_eggs_first": true,
          "min_interval": 120,
          "infinite": [2,5,10],
          "breakable": [2,5,10]
        }
      },
      {
        "type": "TransferPokemon",
        "config": {
          "enabled": true,
          "min_free_slot": 20,
          "transfer_wait_min": 1,
          "transfer_wait_max": 1
        }
      },
      {
        "type": "CatchPokemon",
        "config": {
          "enabled": false,
          "catch_visible_pokemon": true,
          "catch_lured_pokemon": true,
          "catch_incensed_pokemon": true,
          "min_ultraball_to_keep": 5,
          "berry_threshold": 0.35,
          "vip_berry_threshold": 0.9,
          "treat_unseen_as_vip": true,
          "daily_catch_limit": 80000,
          "vanish_settings": {
            "consecutive_vanish_limit": 10,
            "rest_duration_min": "00:30:10",
            "rest_duration_max": "01:00:00"
          },
          "catch_throw_parameters": {
            "excellent_rate": 0.95,
            "great_rate": 0.05,
            "nice_rate": 0.0,
            "normal_rate": 0.0,
            "spin_success_rate" : 0.9,
            "hit_rate": 0.95
          },
          "catch_simulation": {
            "flee_count": 3,
            "flee_duration": 1,
            "catch_wait_min": 1,
            "catch_wait_max": 1,
            "berry_wait_min": 1,
            "berry_wait_max": 1,
            "changeball_wait_min": 1,
            "changeball_wait_max": 1,
            "newtodex_wait_min": 2,
            "newtodex_wait_max": 3
          }
        }
      },
      {
        "type": "EvolvePokemon",
        "config": {
          "enabled": true,
          "evolve_list": [
            "pidgey", "caterpie", "weedle", "rattata",
            "zubat", "venonat", "poliwag", "ekans", "Spearow",
            "// eevee"
          ],
          "donot_evolve_list": "none",

          "first_evolve_by": "cp",
          "evolve_above_cp": 0,
          "evolve_above_iv": 0,
          "logic": "or",
          "min_evolve_speed": 10,
          "max_evolve_speed": 15,
          "min_pokemon_to_be_evolved": 2,
          "use_lucky_egg": false
        }
      },
      {
        "type": "SpinFort",
        "config": {
          "enabled": true,
          "spin_wait_min": 1,
          "spin_wait_max": 2,
          "ignore_item_count": true,
          "daily_spin_limit": 1900000
        }
      },
      {
        "type": "RecycleItems",
        "config": {
          "enabled": true,
          "min_empty_space": 5,
          "max_balls_keep": 350,
          "max_potions_keep": 0,
          "max_berries_keep": 100,
          "max_revives_keep": 0,
          "item_filter": {
            "Pokeball":       { "keep" : 100 },
            "Potion":         { "keep" : 0 },
            "Super Potion":   { "keep" : 0 },
            "Hyper Potion":   { "keep" : 0 },
            "Revive":         { "keep" : 0 },
            "Razz Berry":     { "keep" : 75 }
          },
          "recycle_wait_min": 3,
          "recycle_wait_max": 5,
          "recycle_force": true,
          "recycle_force_min": "00:01:00",
          "recycle_force_max": "00:05:00"
        }
      },
      {
        "type": "MoveToFort",
        "config": {
          "enabled": true,
          "thread": false,
          "lure_attraction": true,
          "lure_max_distance": 2000,
          "walker": "StepWalker",
          "log_interval": 5,
          "wait_on_lure": false,
          "ignore_item_count": true
        }
      },
      {
        "type": "FollowSpiral",
        "config": {
          "enabled": true,
          "diameter": 4,
          "step_size": 70
        }
      }
    ],
    "map_object_cache_time": 5,
    "forts": {
      "avoid_circles": true,
      "max_circle_size": 50,
      "cache_recent_forts": true
    },
    "pokemon_bag": {
      "// if 'show_at_start' is true, it will log all the pokemons in the bag (not eggs) at bot start": {},
      "show_at_start": false,
      "// if 'show_count' is true, it will show the amount of each pokemon (minimum 1)": {},
      "show_count": false,
      "// if 'show_candies' is true, it will show the amount of candies for each pokemon": {},
      "show_candies": false,
      "// 'pokemon_info' parameter define which info to show for each pokemon": {},
      "// the available options are": {},
      "// ['cp', 'iv_ads', 'iv_pct', 'ivcp', 'ncp', 'level', 'hp', 'moveset', 'dps']": {},
      "pokemon_info": ["cp", "iv_pct"]
